                @if (liste != null && liste.Count > 0 && Type_page == "Lgpmc")
                {
                    <div class="section-title" style="align-content:center">
                        <h2>Liste PFE GPMC</h2><br />
                        <p style="font-style:italic; font-weight:bold">
                            Vous trouverez dans le tableau ci-dessous les groupes PFE de la filière GPMC et leurs informations.
                        </p>
                    </div>

                    <table class="table table-striped table-hover table-bordered ">
                        <thead class="thead-light">
                            <tr>

                                <th scope="col" class="col"><i class="fas fa-italic"></i> Groupe ID </th>
                                <th scope="col" class="col"><i class="fas fa-users"></i> Membres </th>
                                <th scope="col" class="col"><i class="fas fa-user-tie"></i> Encadrant Interne </th>
                                <th scope="col" class="col"><i class="fas fa-chalkboard-teacher"></i> Encadrant Externe </th>
                                <th scope="col" class="col"><i class="fas fa-building"></i> Société | Ville </th>
                                <th scope="col" class="col"><i class="fas fa-pen-square"></i> Sujet </th>
                                <th scope="col" class="col"><i class="fas fa-file-pdf"></i> Rapports Avancements </th>
                                <th scope="col" class="col"><i class="fas fa-file-pdf"></i> Rapport Final </th>
                                <th scope="col" class="col"><i class="fas fa-calendar-alt"></i> Date Soutenance </th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var pfe in liste)
                            {
                                <tr>
                                    <td scope="row"> @pfe.Groupe_id </td>
                                    <td>
                                        @{ if (pfe.Membres.Count > 0 && pfe.Membres != null)
                                            { @pfe.Membres[0].nom @espace @pfe.Membres[0].prenom }
                                        } <br />
                                        @{ if (pfe.Membres.Count > 1 && pfe.Membres != null)
                                            { @pfe.Membres[1].nom @espace @pfe.Membres[1].prenom }
                                        }
                                    </td>
                                    <td> @pfe.Encadrant_interne</td>
                                    <td> @pfe.Encadrant_externe</td>
                                    <td> @pfe.Societe @pfe.Ville</td>
                                    <td> @pfe.Sujet</td>
                                    <td>
                                        @{ if (pfe.Rapports_avancement.Count > 0)
                                            {
                                                if (pfe.Rapports_avancement[0] != null)
                                                {<span class="badge badge-light">@Html.ActionLink("Rapport_Avancement_1", "GetFile", "Enseignant", new { Id = pfe.Rapports_avancement[0].Id })</span>
                                            }
                                            else
                                            {@mes}
                                    } }<br>
                                        @{ if (pfe.Rapports_avancement.Count > 1)
                                            {
                                                if (pfe.Rapports_avancement[1] != null)
                                                {<span class="badge badge-light">@Html.ActionLink("Rapport_Avancement_2", "GetFile", "Enseignant", new { Id = pfe.Rapports_avancement[1].Id })</span>
                                            }
                                            else
                                            {@mes}
                                    } }<br>
                                        @{ if (pfe.Rapports_avancement.Count > 2)
                                            {
                                                if (pfe.Rapports_avancement[2] != null)
                                                {<span class="badge badge-light">@Html.ActionLink("Rapport_Avancement_3", "GetFile", "Enseignant", new { Id = pfe.Rapports_avancement[2].Id })</span>
                                            }
                                            else
                                            {@mes}
                                    } } <br>
                                        @{ if (pfe.Rapports_avancement.Count > 3)
                                            {
                                                if (pfe.Rapports_avancement[3] != null)
                                                {<span class="badge badge-light">@Html.ActionLink("Rapport_Avancement_4", "GetFile", "Enseignant", new { Id = pfe.Rapports_avancement[3].Id })</span>
                                            }
                                            else
                                            {@mes}
                                    } }
                                    </td>
                                    <td>
                                        @{ if (pfe.Rapport_final != null)
                                            {<span class="badge badge-light">@Html.ActionLink("Rapport_Final", "GetFile", "Enseignant", new { Id = pfe.Rapport_final.Id })</span>
                                        }
                                        else
                                        { <span class="badge badge-warning">Aucun rapport</span> } }
                                    </td>
                                    <td> @pfe.Date_soutenance</td>

                                </tr>

                            }
                        </tbody>
                    </table>
                }

                @if (Type_page == "planning")
                {
                    <div class="section-title" style="align-content:center">
                        <h2>Planning</h2><br />
                        <p style="font-style:italic; font-weight:bold">
                            Vous trouverez dans cette page un lien pour telecharger le planning des soutenances.
                        </p>
                    </div>
                    <i class="fas fa-download" style="color:cornflowerblue"></i>
                    @Html.ActionLink("Télécharger le planning", "GetFilePlanning", "Enseignant"); <br />
                    <span class="badge badge-warning">@messagePlanning</span>

                }
                <!--La fin du code c# razor-->
            </div>
        </div>
    </div>
</div>
<!--</div>-->
